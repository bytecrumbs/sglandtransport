    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug mode",
            "request": "launch",
            "type": "dart",
            "toolArgs": [
                "--dart-define-from-file",
                "api-keys.json"
            ],
        },
        {
            "name": "Integration Test: Run Test",
            "program": "project_root/integration_test/foo_test.dart",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "profile mode",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "release mode",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}